{
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.1"
    },
    "*.php": {
      "runtime": "vercel-php@0.7.1"
    },
    "admin/*.php": {
      "runtime": "vercel-php@0.7.1"
    }
  },
  "routes": [
    { "src": "/admin/(.*)", "dest": "/admin/$1" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
