{
  "builds": [
    {
      "src": "**/*.php",
      "use": "vercel-php@0.7.1"
    }
  ],
  "routes": [
    { "src": "/admin/(.*)", "dest": "/admin/$1" },
    { "src": "/about",      "dest": "/about.php" },
    { "src": "/contact",    "dest": "/contact.php" },
    { "src": "/membership", "dest": "/membership.php" },
    { "src": "/",           "dest": "/index.php" }
  ],
  "env": {
    "APP_ENV": "production"
  }
}